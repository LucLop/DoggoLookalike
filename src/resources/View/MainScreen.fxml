   <children>
      <ImageView fx:id="menuImg" fitHeight="400.0" fitWidth="556.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../menu.jpg" />
         </image>
      </ImageView>
      <Button id="dogButton" fx:id="dogButton" layoutX="99.0" layoutY="200.0" mnemonicParsing="false" onAction="#handleDogButtonClick" text="Dog" />
