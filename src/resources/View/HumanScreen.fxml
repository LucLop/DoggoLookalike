      <Label layoutX="199.0" layoutY="27.0" text="Welcome to the Human dog lookalike" />
      <Label fx:id="breedLabel" layoutX="300.0" layoutY="336.0" />
      <Button fx:id="validateButton" layoutX="87.0" layoutY="275.0" mnemonicParsing="false" onAction="#humanDogLookalikeRecognition" text="Validate" />
      <Button layoutX="520.0" layoutY="345.0" mnemonicParsing="false" onAction="#returnMenu" text="Return" />
      <ImageView fx:id="humanImg" fitHeight="150.0" fitWidth="200.0" layoutX="17.0" layoutY="97.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="dogImg" fitHeight="150.0" fitWidth="200.0" layoutX="372.0" layoutY="97.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
