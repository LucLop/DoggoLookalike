<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.DogController">
   <children>
      <ImageView fx:id="dogImg" fitHeight="238.0" fitWidth="352.0" layoutX="124.0" layoutY="81.0" onDragDropped="#handleImageDrop" onDragOver="#handleImageDragOver" pickOnBounds="true" preserveRatio="true" />
      <Label layoutX="171.0" layoutY="34.0" text="Welcome to the Dog Breed Recognition" />
      <Button fx:id="validateButton" layoutX="264.0" layoutY="335.0" mnemonicParsing="false" onAction="#dogBreedRecognition" text="Validate" />
      <Button layoutX="526.0" layoutY="349.0" mnemonicParsing="false" onAction="#returnMenu" text="Return" />
      <Text fx:id="dogBreed" layoutX="29.0" layoutY="323.0" strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="220.0" />
   </children>
</AnchorPane>
